    "graph": {
        "nodes": [
            {
                "id": "5.1",
                "type": "axiom",
                "name": "Relation between primed and unprimed predicates",
                "complexity": "moderate",
                "pattern": "axiom_schema",
                "defeasible": false,
                "reified": true,
                "fol": "(forall (x) (iff (p x) (exists (e)(and (p' e x)(Rexist e)))))",
                "english": "p is true of x if and only if there is an eventuality e that is the eventuality of p being true of x and e really exists",
                "group": "axiom_moderate"
            },
            {
                "id": "5.2",
                "type": "axiom",
                "name": "Running generates going",
                "complexity": "moderate",
                "pattern": "generation_rule",
                "defeasible": false,
                "reified": true,
                "fol": "(forall (e1 x) (if (run' e1 x) (exists (e2) (and (go' e2 x)(gen e1 e2)))))",
                "english": "Whenever a running event e1 occurs, there is a going event e2 that it generates",
                "group": "axiom_moderate"
            },
            {
                "id": "5.3",
                "type": "axiom",
                "name": "First argument of give predicate",
                "complexity": "simple",
                "pattern": "argument_structure",
                "defeasible": false,
                "reified": true,
                "fol": "(forall (e x y z) (if (give' e x y z)(argn x 1 e)))",
                "english": "If e is an eventuality of x giving y to z, then x is the first argument of e",
                "group": "axiom_simple"
            },
            {
                "id": "5.4",
                "type": "axiom",
                "name": "Second argument of give predicate",
                "complexity": "simple",
                "pattern": "argument_structure",
                "defeasible": false,
                "reified": true,
                "fol": "(forall (e x y z) (if (give' e x y z)(argn y 2 e)))",
                "english": "If e is an eventuality of x giving y to z, then y is the second argument of e",
                "group": "axiom_simple"
            },
            {
                "id": "5.5",
                "type": "axiom",
                "name": "Third argument of give predicate",
                "complexity": "simple",
                "pattern": "argument_structure",
                "defeasible": false,
                "reified": true,
                "fol": "(forall (e x y z) (if (give' e x y z)(argn z 3 e)))",
                "english": "If e is an eventuality of x giving y to z, then z is the third argument of e",
                "group": "axiom_simple"
            },
            {
                "id": "5.6",
                "type": "axiom",
                "name": "Eventuality is self-argument",
                "complexity": "simple",
                "pattern": "self_reference",
                "defeasible": false,
                "reified": true,
                "fol": "(forall (e x y z) (if (give' e x y z)(argn e 0 e)))",
                "english": "If e is an eventuality of giving, then e is the 0th argument of itself",
                "group": "axiom_simple"
            },
            {
                "id": "5.7",
                "type": "axiom",
                "name": "Constraints on argn arguments",
                "complexity": "simple",
                "pattern": "type_constraint",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (e x n) (if (argn x n e)(and (nonNegInteger n)(eventuality e))))",
                "english": "If x is the nth argument of e, then n is a nonnegative integer and e is an eventuality",
                "group": "axiom_simple"
            },
            {
                "id": "5.8",
                "type": "axiom",
                "name": "Definition of eventuality",
                "complexity": "simple",
                "pattern": "definition",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (e)(iff (eventuality e)(argn e 0 e)))",
                "english": "Something is an eventuality if and only if it is the 0th argument of itself",
                "group": "axiom_simple"
            },
            {
                "id": "5.9",
                "type": "axiom",
                "name": "Definition of arg predicate",
                "complexity": "moderate",
                "pattern": "existential_definition",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (e x) (iff (arg x e)(exists (n)(argn x n e))))",
                "english": "x is an argument of e if it is the nth argument for some n",
                "group": "axiom_moderate"
            },
            {
                "id": "5.10",
                "type": "axiom",
                "name": "Definition of arg* predicate",
                "complexity": "complex",
                "pattern": "recursive_definition",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (x e1) (iff (arg* x e1) (or (arg x e1) (exists (e2)(and (arg e2 e1)(arg* x e2))))))",
                "english": "x is an arg* of e1 if x is an argument of e1, or an argument of an argument of e1, and so on",
                "group": "axiom_complex"
            },
            {
                "id": "5.11",
                "type": "axiom",
                "name": "Predicate of give eventuality",
                "complexity": "simple",
                "pattern": "predicate_assignment",
                "defeasible": false,
                "reified": true,
                "fol": "(forall (e x y z) (if (give' e x y z)(pred give e)))",
                "english": "If e is a giving event by x of y to z, then the entity we call give is the pred of e",
                "group": "axiom_simple"
            },
            {
                "id": "5.12",
                "type": "axiom",
                "name": "Pred type constraints",
                "complexity": "simple",
                "pattern": "type_constraint",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (e p) (if (pred p e)(and (predicate p)(eventuality e))))",
                "english": "The pred of an eventuality is a predicate and eventualities are eventualities",
                "group": "axiom_simple"
            },
            {
                "id": "5.13",
                "type": "axiom",
                "name": "Arity of give predicate",
                "complexity": "simple",
                "pattern": "arity_assignment",
                "defeasible": false,
                "reified": true,
                "fol": "(forall (e x y z) (if (give' e x y z)(arity 3 e)))",
                "english": "A giving event has an arity of 3",
                "group": "axiom_simple"
            },
            {
                "id": "5.14",
                "type": "axiom",
                "name": "Arity type constraints",
                "complexity": "simple",
                "pattern": "type_constraint",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (n e) (if (arity n e)(and (nonNegInteger n)(eventuality e))))",
                "english": "The arity of an eventuality is a nonnegative integer and eventualities are eventualities",
                "group": "axiom_simple"
            },
            {
                "id": "5.15",
                "type": "axiom",
                "name": "Complete structure of eventualities",
                "complexity": "complex",
                "pattern": "complete_definition",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (e) (iff (eventuality e) (exists (p n) (and (pred p e)(arity n e) (forall (i) (if (and (posInteger i) (leq i n)) (exists (x) (argn x i e))))))))",
                "english": "If e is an eventuality, then it has a predicate p, an arity n and n arguments",
                "group": "axiom_complex"
            },
            {
                "id": "5.16",
                "type": "axiom",
                "name": "Gen relation between eventualities",
                "complexity": "simple",
                "pattern": "type_constraint",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (e1 e2) (if (gen e1 e2)(and (eventuality e1)(eventuality e2))))",
                "english": "The gen relation is between two eventualities",
                "group": "axiom_simple"
            },
            {
                "id": "5.17",
                "type": "axiom",
                "name": "Gen is antireflexive",
                "complexity": "simple",
                "pattern": "antireflexivity",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (e) (not (gen e e)))",
                "english": "The gen relation is antireflexive - an eventuality does not generate itself",
                "group": "axiom_simple"
            },
            {
                "id": "5.18",
                "type": "axiom",
                "name": "Gen modus ponens with Rexist",
                "complexity": "simple",
                "pattern": "modus_ponens",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (e1 e2) (if (and (gen e1 e2)(Rexist e1)) (Rexist e2)))",
                "english": "If e1 generates e2 and e1 really exists, then e2 really exists",
                "group": "axiom_simple"
            },
            {
                "id": "5.19",
                "type": "axiom",
                "name": "Gen is stronger than implication",
                "complexity": "simple",
                "pattern": "existence_claim",
                "defeasible": false,
                "reified": false,
                "fol": "(exists (e1 e2) (and (imply e1 e2)(not (gen e1 e2))))",
                "english": "Generation is stronger than implication - there exist events where one implies another but doesn't generate it",
                "group": "axiom_simple"
            },
            {
                "id": "5.20",
                "type": "axiom",
                "name": "Gen preserves temporal location",
                "complexity": "moderate",
                "pattern": "temporal_preservation",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (e1 e2 t) (if (gen e1 e2)(iff (atTime e1 t)(atTime e2 t))))",
                "english": "If e1 generates e2, then e1 and e2 occur at the same time",
                "group": "axiom_moderate"
            },
            {
                "id": "5.21",
                "type": "axiom",
                "name": "Gen preserves spatial location",
                "complexity": "moderate",
                "pattern": "spatial_preservation",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (e1 e2 x s) (if (gen e1 e2)(iff (atLoc e1 x s)(atLoc e2 x s))))",
                "english": "If e1 generates e2, then e1 and e2 occur at the same location",
                "group": "axiom_moderate"
            },
