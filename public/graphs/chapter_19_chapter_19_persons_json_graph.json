    "graph": {
        "nodes": [
            {
                "id": "19.1",
                "type": "axiom",
                "name": "Person is Agent",
                "complexity": "simple",
                "pattern": "type_constraint",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (p) (if (person p)(agent p)))",
                "english": "A person is a kind of agent.",
                "group": "axiom_simple"
            },
            {
                "id": "19.2",
                "type": "axiom",
                "name": "Person is Physical Object",
                "complexity": "simple",
                "pattern": "type_constraint",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (p) (if (person p)(physobj p)))",
                "english": "A person is also a kind of physical object.",
                "group": "axiom_simple"
            },
            {
                "id": "19.3",
                "type": "axiom",
                "name": "Person Has Body and Mind",
                "complexity": "simple",
                "pattern": "existence_claim",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (p) (if (person p) (exists (b m)(and (body b p)(mind m p)))))",
                "english": "A person has a body and a mind.",
                "group": "axiom_simple"
            },
            {
                "id": "19.4",
                "type": "axiom",
                "name": "Mind is Composite Entity",
                "complexity": "simple",
                "pattern": "type_constraint",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (m p) (if (mind m p) (compositeEntity m)))",
                "english": "A mind is a composite entity, allowing us to talk about entities external to the mind.",
                "group": "axiom_simple"
            },
            {
                "id": "19.5",
                "type": "axiom",
                "name": "Body is Physical Object",
                "complexity": "simple",
                "pattern": "type_constraint",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (b p) (if (body b p)(physobj b)))",
                "english": "A body is a physical object.",
                "group": "axiom_simple"
            },
            {
                "id": "19.6",
                "type": "axiom",
                "name": "Body is Spatial System",
                "complexity": "moderate",
                "pattern": "definition",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (b p) (if (body b p) (and (spatialSystem b) (exists (s) (and (componentsOf s b) (forall (x) (iff (member x s)(bodyPart x p))))))))",
                "english": "A body is a spatial system whose components are body parts, since all body parts are physical objects.",
                "group": "axiom_moderate"
            },
            {
                "id": "19.7",
                "type": "axiom",
                "name": "Body Parts Set Definition",
                "complexity": "moderate",
                "pattern": "definition",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (p s) (iff (bodyPartsOf s p) (exists (b) (and (person p)(body b p)(componentsOf s b)))))",
                "english": "A person p has a set of body parts, namely, the components of the body.",
                "group": "axiom_moderate"
            },
            {
                "id": "19.8",
                "type": "axiom",
                "name": "Perceive Argument Constraints",
                "complexity": "simple",
                "pattern": "type_constraint",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (a x m) (if (and (perceive a x)(mind m a)) (and (agent a)(externalTo x m))))",
                "english": "Perception is a relation between an agent and an entity or eventuality external to the mind.",
                "group": "axiom_simple"
            },
            {
                "id": "19.9",
                "type": "axiom",
                "name": "Nearness Enables Perception",
                "complexity": "moderate",
                "pattern": "enabling_condition",
                "defeasible": false,
                "reified": true,
                "fol": "(forall (p x e2) (if (perceive' e2 p x) (exists (e1 s) (and (near' e1 x p s)(enable e1 e2)))))",
                "english": "Something being near a person is an enabling condition for perceiving it.",
                "group": "axiom_moderate"
            },
            {
                "id": "19.10",
                "type": "axiom",
                "name": "Sense Organs Subset",
                "complexity": "moderate",
                "pattern": "definition",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (p x) (if (and (person p)(bodyPartsOf s1 p)) (exists (s2) (and (subset s2 s1) (forall (o) (iff (member o s2)(senseOrgan o p)))))))",
                "english": "The sense organs are a subset of the body parts.",
                "group": "axiom_moderate"
            },
            {
                "id": "19.11",
                "type": "axiom",
                "name": "Sense Organ is Body Part",
                "complexity": "simple",
                "pattern": "logical_implication",
                "defeasible": false,
                "reified": false,
                "fol": "(forall (o p) (if (senseOrgan o p)(bodyPart o p)))",
                "english": "It follows that sense organs are body parts.",
                "group": "axiom_simple"
            },
            {
                "id": "19.12",
                "type": "axiom",
                "name": "Intact Sense Organ Enables Perception",
                "complexity": "moderate",
                "pattern": "enabling_condition",
                "defeasible": false,
                "reified": true,
                "fol": "(forall (p x e2) (if (and (person p)(perceive' e2 p x)) (exists (o e1) (and (senseOrgan o p)(intact' e1 o)(enable e1 e2)))))",
                "english": "When something is perceived, there is a sense organ whose intact-ness enables the perception.",
                "group": "axiom_moderate"
            },
            {
                "id": "19.13",
                "type": "axiom",
                "name": "Voluntary Body Part Control",
                "complexity": "complex",
                "pattern": "definition",
                "defeasible": false,
                "reified": true,
                "fol": "(forall (p s1) (if (and (person p)(bodyPartsOf s1 p)) (exists (s2) (and (subset s2 s1) (forall (x) (if (member x s2) (exists (e1 e2 y z) (and (move' e2 x y z)(will' e1 p e2) (dcause e1 e2)))))))))",
                "english": "A subset of body parts can be directly controlled by a person's will - the person's willing an event is the direct cause of the motion of the body part.",
                "group": "axiom_complex"
            },
            {
                "id": "19.14",
                "type": "axiom",
                "name": "Voluntary Arm Lifting Example",
                "complexity": "moderate",
                "pattern": "example",
                "defeasible": false,
                "reified": true,
                "fol": "(forall (p) (if (person p) (exists (e1 e2 x) (and (lift' e2 p x)(arm x p)(will' e1 p e2) (dcause e1 e2)))))",
                "english": "Example: a person can voluntarily lift his or her arm - the lifting is directly caused by willing it.",
                "group": "axiom_moderate"
            },
